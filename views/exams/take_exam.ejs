<!DOCTYPE html>
<html lang="en">
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
  <script src="/javascripts/question_list.js"></script>
  <script type="text/javascript">var exam_code =JSON.stringify(exam_code);</script>
  <script src="/javascripts/question_list.js"></script>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <Take>Exam</Take>
    <!-- CSS-->
    <link rel="stylesheet" href="/stylesheets/assets/bootstrap/css/font.css">
    <link rel="stylesheet" href="/stylesheets/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/assets/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/stylesheets/assets/css/form-elements.css">
    <link rel="stylesheet" href="/stylesheets/assets/css/style.css">
  </head>
  <body>
    <!-- Top content-->
    <div class="top-content">
      <div class="inner-bg">
        <div class="container">
          <div class="row">
            <div class="col-sm-8 col-sm-offset-2 text">
              <h1><strong>Exam Code:<%= exam_code %> </strong>&nbsp &nbsp &nbsp &nbsp &nbsp 
                <h1 id="time"></h1>
              </h1>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-8 form-box">
              <div class="form-top">
                <div class="form-top-left">
                  <h3>Questions</h3><br>
                </div>
              </div>
              <div class="form-bottom">
                <p>
                  <h4 id="number"><%= exam.question[0].question %> </h4><span id="question"></span><br><strong>A:  <%= exam.question[0].optionA %>              </strong><span id="optionA"></span>&nbsp &nbsp &nbsp &nbsp &nbsp<strong>B:   <%= exam.question[0].optionB %>              </strong><span id="optionB" align="right"></span><br><strong>C:   <%= exam.question[0].optionC %>           </strong><span id="optionC"></span>&nbsp &nbsp &nbsp &nbsp &nbsp<strong>D:  <%= exam.question[0].optionD %>              </strong><span id="optionD"></span>
                </p>
              </div>
            </div>
            <div class="col-sm-4 form-box">
              <div class="form-top">
                <div class="form-top-left">
                  <h3>Questions</h3><br>
                </div>
              </div>
              <div class="form-bottom">
                <form role="takeexam" action="submit" method="post" id="formResponse" class="form-response">
                  <div id="questionList">
                    <table>
                      <thead>
                        <th>Question No.</th>
                        <th>Choice</th>
                      </thead>
                      <tbody></tbody>
                    </table>
                  </div><br><br>
                  <div class="form-group">
                    <label for="form-exam_code" class="sr-only">exam_code</label>
                    <input id="form-exam_code" type="hidden" name="exam_code" class="form-exam_code form-control" value=<%= exam.exam_code %> >
                  </div><br>
                  <div class="form-group">
                    <label for="form-username" class="sr-only">username</label>
                    <input id="form-username" type="hidden" name="username" class="form-username form-control" value=<%= exam.username %>>
                  </div>
                  <button type="submit" class="btn">Submit</button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Javascript-->
    <script src="/stylesheets/assets/js/jquery-1.11.1.min.js"></script>
    <script src="/stylesheets/assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="/stylesheets/assets/js/jquery.backstretch.min.js"></script>
    <script src="/stylesheets/assets/js/scripts.js"></script>
    <!--if lt IE 10script(src='assets/js/placeholder.js')
    -->
  </body>
</html>